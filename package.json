{
  "name": "fear-core-build",
  "version": "1.1.4",
  "description": "FEAR core API driven build tasks",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint test/ tasks/",
    "test": "mocha 'test/**/*.spec.js'",
    "pretest": "npm run eslint",
    "watch:test": "onchange test/**/*.js tasks/**/*.js -- npm run test -s",
    "dev": "npm run watch:test -s",
    "docs": "./node_modules/.bin/jsdoc ./tasks index.js --recurse --destination docs --readme README.md",
    "prepublish" : "npm test",
    "postpublish": "git checkout gh-pages; git merge master --strategy-option theirs; npm run docs; git add .; git commit -m 'update jsdocs'; git push upstream gh-pages; git checkout master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DigitalInnovation/fear-core-build.git"
  },
  "author": {
    "name": "Alex Bowen",
    "email": "alex.bowen@mnscorp.net"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DigitalInnovation/fear-core-build/issues"
  },
  "homepage": "http://digitalinnovation.github.io/fear-core-build",
  "dependencies": {
    "autoprefixer": "6.1.1",
    "del": "1.2.0",
    "gulp-batch": "1.0.5",
    "gulp-copy": "0.0.2",
    "gulp-flatten": "0.0.4",
    "gulp-header": "1.2.2",
    "gulp-if": "1.2.5",
    "gulp-inline-source": "1.2.0",
    "gulp-minify-css": "0.5.1",
    "gulp-minify-html": "1.0.4",
    "gulp-minify-inline": "0.1.1",
    "gulp-ng-annotate": "1.1.0",
    "gulp-postcss": "6.0.1",
    "gulp-remove-code": "1.0.2",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.3",
    "gulp-sass": "2.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.4.1",
    "gulp-util": "3.0.6",
    "jspm": "0.16.10",
    "run-sequence": "1.1.4",
    "yargs": "3.30.0"
  },
  "devDependencies": {
    "jsdoc": "3.4.0",
    "onchange": "2.0.0"
  }
}
